    width: auto !important;
    display: flex !important;
    justify-content: center !important;
    align-items: center !important;
    padding-left: 0.25rem !important;
    padding-right: 0.25rem !important;
    margin: 0 auto !important;
  }

  /* Ensure the link inside stacks vertically and is centered */
  .topbar .col-8 > a,
  .topbar .col-8 > a.text-decoration-none {
    display: flex !important;
    flex-direction: column !important;
    align-items: center !important;
    justify-content: center !important;
    width: auto !important;
    padding: 0 !important;
    margin: 0 !important;
    text-align: center !important;
  }

  /* Kill any inline margin-left on the img and make it block-centered */
  .topbar .col-8 img,
  .topbar .col-8 img.site-logo,
  a.brand-link img {
    margin-left: 0 !important;
    margin-right: 0 !important;
    margin: 0 auto !important;
    display: block !important;
    width: auto !important;
    max-width: 86% !important;    /* responsive: won't overflow on tiny screens */
    height: auto !important;
    object-fit: contain !important;
  }

  /* Brand inner wrapper: center text and reduce spacing */
  .topbar .col-8 a > div,
  .brand-inner {
    display: flex !important;
    flex-direction: column !important;
    align-items: center !important;
    justify-content: center !important;
